friends = ["Alice", "Bob", "Charlie", "David", "Emanuel"]
import random
num = random.randint(0,4)
if num == 0:
    print(friends[0])
elif num == 1:
    print(friends[1])
elif num == 2:
    print(friends[2])
elif num == 3:
    print(friends[3])
else:
    print(friends[4]) 

